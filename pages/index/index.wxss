/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}


.journey-container{
  margin-top: 300rpx;
  border: 1px solid #1296db;
  width: 200rpx;
  height: 80rpx;
  border-radius:5px;
  text-align: center; 
}

.journey{
  font-size: 22rpx;
  font-weight: bold;
  line-height: 80rpx;
  color: #1296db
}